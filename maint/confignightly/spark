#! /bin/csh
#
set MACH=spark
set USR=petsc
set LOC=/sandbox/petsc/petsc-dev
set TAO_LOC=/sandbox/petsc/tao-devel
set CONF=solaris2.9
if ($day == Tue || $day == Thu) then
  set CONF=solaris-gnu
else if ($day == Mon || $day == Sat) then
  set CONF=solaris-lam
else if ($day == Sun) then
  set CONF=solaris-64
endif
