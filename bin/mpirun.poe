#!/usr/local/bin/bash
#
# This script is for IBM SP machines running POE that do not 
# have mpirun installed. 
#
# Usage: bin/mpirun.poe [-np n] programname programoptions
#
two=$2
if [ $1 = "-np" ]; then
        shift
        shift
        /usr/bin/poe $* -procs $two
else
        /usr/bin/poe $*
fi

